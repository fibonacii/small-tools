<%- include('../components/header') %>
<script type="text/javascript" src="../../public/js/libs/rsa/jsencrypt.js"></script>
<script type="text/javascript" src="../../public/js/base/baseInput.js"></script>
<script type="text/javascript" src="../../public/js/user/login.js"></script>

<body background=../../public/img/background/bacgrd3.jpg >
<div class="container">
    <h1 >这是登录页</h1>
    <p id="notification"></p>
    <div class="row myCenter">
        <div class="col-md-offset-4">
            <form class="form-horizontal">
                <div class="form-group ">
                    <div class="col-lg-5">
                       <label  class="control-label">邮箱:</label>
                       <input type="text" class="form-control input-lg" id="email" placeholder="请输入注册邮箱"/>
                    </div>
                </div>
                <div class="form-group ">
                    <div class="col-lg-5">
                        <label  for="passWord">密码:</label>
                        <input type="password" id="passWord" class="form-control input-lg" placeholder="请输入密码"/>
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-sm-5 ">
                        <button type="button" class="btn btn-primary btn-block " id="login">登 录</button>
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-lg-offset-2">
                        <text>没有账号，去注册>>></text>
                        <a href="/user/register" class="btn btn-primary" id="register">注册</a>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
</body>
<%- include('../components/footer') %>
<%- include('../../components/header') %>
<link rel="stylesheet" href="../../../public/support/jRange/jquery.range.css"/>
<script type="text/javascript" src="../../../public/support/jRange/jquery.range.js"></script>
<script type="text/javascript" src="../../../public/js/apps/score/vote.js"></script>
</head>
<body>
<div id="scoreInformation" scoreId=<%= score._id %>></div>
<div>
    <label>标题：</label>
    <p style="margin-left: 10px"><%- score.scoreName %></p>
</div>
<div>
    <label>描述:</label>
    <div><%- score.scoreOverView %></div>
</div>
<div>
    <label>链接:</label>
    <div><%- score.scoreLink %></div>
</div>
<div id="scoreNums" scoreFromNum=<%= score.lowScore%> scoreToNum=<%= score.highScore%>></div>
<p></p>
<div style="width: 80%;margin-left: 100px">
    <input type="hidden" id="scoreSlide" class="single-slider" />
</div>
<div>
    <button type="button" id="submitScoreVote" class="btn btn-success">提交</button>
</div>


<%- include('../../components/footer') %>
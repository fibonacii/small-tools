<%- include('../components/header') %>
<script type="text/javascript" ></script>

<% /* 右半侧弄个花哨的图挂上去，后面再看 */ %>
<div class="container">
    <h1>这是注册页</h1>
    <form class="form-horizontal">
        <div class="form-group ">
            <div class="col-lg-4">
            <label class="control-label">用户名:</label>
               <input type="text" class="form-control input-lg" id="userName" placeholder="请输入用户名"/>
            </div>
        </div>
        <div class="form-group ">
            <div class="col-lg-4">
            <label for="passWord">密码:</label>
            <input type="password" id="passWord" class="form-control input-lg" placeholder="请输入密码"/>
            </div>
        </div>
        <div class="form-group ">
            <div class="col-lg-4">
            <label for="checkAlpha">验证码：</label>
            <input type="text" id="checkAlpha" class="form-control input-lg" placeholder="请输入验证码">
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-2 ">
                <button type="button" class="btn btn-primary btn-block " id="sign_up">注册</button>
            </div>
            <div class="col-sm-2 ">
                <button type="button" class="btn btn-primary btn-block " id="login">登录</button>
            </div>
        </div>

    </form>
</div>
<%- include('../components/footer') %>